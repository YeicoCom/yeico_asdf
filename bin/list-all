#!/usr/bin/env bash

# response is json with each property in a separated line: "name": "test",
tags_url=https://api.github.com/repos/YeicoCom/yeico_cli/tags
tags_list=$(curl -s $tags_url | grep -oP '\"name\": \"\K[^\"]+' | LC_ALL=C sort)
echo main $tags_list

